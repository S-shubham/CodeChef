//ATM pin problem codechef

#include<bits/stdc++.h>
using namespace std;

int main()
{
		int T;
		cin>>T;
		for(int t=0;t<T;t++)
		{
			int n;
			cin>>n;
			int q=0;
			cout<<1<<" ";
			cout<<1;
			if(n%2==0)
			q=n/2;
			else
			q=(n/2);
			for(int i=0;i<q;i++)
			cout<<0;
			
			cout<<"\n";
			
		}
}
